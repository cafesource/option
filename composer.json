{
  "name": "cafesource/option",
  "type": "library",
  "description": "Laravel options",
  "keywords": [
    "cafesource",
    "option",
    "laravel",
    "laravel-option"
  ],
  "license": "MIT",
  "homepage": "https://github.com/cafesource/option",
  "authors": [
    {
      "name": "Mahdi akbari",
      "email": "mahdi75akbari@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3 || ^8.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "cafesource/foundation": "^1.0"
  },
  "provide": {
    "psr/container-implementation": "1.0"
  },
  "autoload": {
    "files": [
      "helpers/option.php"
    ],
    "psr-4": {
      "Cafesource\\Option\\": "src/"
    }
  },
  "autoload-dev": {
    "files": []
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
